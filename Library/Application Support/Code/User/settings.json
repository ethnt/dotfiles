{
  "workbench.colorTheme": "Braver's Solarized Dark (themed workbench)",
  "editor.fontFamily": "MonoLisa, Times, Operator Mono Lig, mono",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.formatOnPaste": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "ruby.useBundler": true,
  "ruby.interpreter.commandPath": "/Users/ethan/.asdf/shims/ruby",
  "ruby.lint": {
    "rubocop": {
      "command": "bundle exec rubocop",
      "lint": true
    }
  },
  "editor.lineHeight": 25,
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "editor.rulers": [
    120
  ],
  "elixirLS.dialyzerFormat": "dialyxir_short",
  "window.zoomLevel": 0,
  "elixirLinter.useStrict": true,
  "explorer.compactFolders": false
}
